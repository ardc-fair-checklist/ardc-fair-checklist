{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-essential",
        "plugin:@typescript-eslint/recommended",
        "airbnb"
    ],
    "ignorePatterns": [
        "**/*.css",
        "**/*.json",
        "**/*.md"
    ],
    "overrides": [
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "parser": {
            "ts": "@typescript-eslint/parser",
            "<template>": "espree"
        }
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "reportUnusedDisableDirectives": true,
    "rules": {
    }
}
